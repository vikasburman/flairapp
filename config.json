{
    "source": {
        "sys": "gears/modules/",
        "app": "app/modules/",
        "web": "web/modules/",
        "www": {
            "sys": "gears/web/",
            "web": "web/"
        },
        "exclude": [
            "assets/**",
            "libs/**",
            "tests/**"
        ],
        "uglify": {
            "js": {
                "mangle": false,
                "preserveComments": "license",
                "output": {
                },
                "compress": {
                    "dead_code": true,
                    "drop_debugger": true,
                    "global_defs": {
                    }
                }
            }
        }
    },

    "catalog": {
        "Base": "sys.core.Base",
        "Boot": "sys.core.boot",
        "Main": "app.main | web.main"
    },

    "weaving": {

    },

    "settings": {
        "sys.core": {
            "bootwares": [
                "sys.Server | sys.Client",
                "app.main | web.main"
            ]
        }
    }
}