{
    "flair.app": {
        "app": "myapp.main.App",

        "boot": {
            "files": [
            ],
            "preambles": [
                "./app/preamble.js"
            ],
            "bootwares": [
                "flair.boot.DIContainer",
                "flair.boot.vue.VueSetup",
                "flair.boot.ClientRouter"
            ]
        },

        "di": {
            "container": {
            }
        },

        "client": {
            "view": {
                "el": "main",
                "title": "My App",
                "transition": ""
            },        
    
            "url": {
                "hashbang": false,
                "i18n": false,
                "routes": {
                    "home": "myapp.main.ui.home",
                    "404": "myapp.main.ui.notfound"
                }
            },
    
            "i18n": {
                "enabled": true,
                "locale": "en",
                "locales": [
                    { "code": "en", "name": "English", "native": "English" }
                ]
            },
    
            "routing": {
                "mounts": {
                    "main": "/"
                },
                "main-options": [
                    { "name": "strict", "value": false }
                ],
                "main-interceptors": [
                ]
            }
        }
    },
    "worker:flair.app": {
        "boot": {
            "files": [
            ],
            "preambles": [
                "./app/preamble.js"
            ],
            "bootwares": [
                "flair.boot.DIContainer"
            ]
        }
    },

    "flair.vue": {
        "extensions": []
    }
}