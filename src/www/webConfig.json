{
    "flair.app": {
        "boot": {
            "load": [
                "./app/preamble.js",
                "flair.boot.DIContainer",
                "flair.ui.vue.VueSetup",
                "flair.boot.ClientRouter"
            ]
        },

        "di": {
            "container": {
            }
        }
    },
    "worker:flair.app": {
        "boot": {
            "load": [
                "./app/preamble.js",
                "flair.boot.DIContainer"
            ]
        }
    },

    "flair.client": {
        "view": {
            "el": "main",
            "title": "My App",
            "transition": ""
        },        

        "vue": {
            "components": [],
            "filters": [],
            "directives": [],
            "mixins": [],
            "plugins": [],
            "pluginOptions": {
            }
        },

        "url": {
            "hashbang": false,
            "i18n": false,
            "404": "/404"
        },

        "i18n": {
            "enabled": true,
            "locale": "en",
            "locales": [
                { "code": "en", "name": "English", "native": "English" }
            ]
        },

        "routing": {
            "mounts": {
                "main": "/"
            },
            "main-options": [
                { "name": "strict", "value": false }
            ],
            "main-interceptors": [
            ]
        }
    }
  
}