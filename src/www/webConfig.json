{
    "flair.app": {
        "boot": {
            "load": [
                "./app/preamble.js",
                "flair.boot.DIContainer",
                "flair.ui.vue.VueSetup",
                "flair.boot.ClientRouter"
            ]
        },

        "di": {
            "container": {
            }
        },

        "client": {
            "view": {
                "el": "main",
                "title": "My App",
                "transition": ""
            },        
    
            "url": {
                "hashbang": false,
                "i18n": false,
                "404": "/404"
            },
    
            "vue": {
                "components": [],
                "filters": [],
                "directives": [],
                "mixins": [],
                "plugins": [],
                "pluginOptions": {
                }
            },
    
            "i18n": {
                "enabled": true,
                "locale": "en",
                "locales": [
                    { "code": "en", "name": "English", "native": "English" }
                ]
            },
    
            "routing": {
                "mounts": {
                    "main": "/"
                },
                "main-options": [
                    { "name": "strict", "value": false }
                ],
                "main-interceptors": [
                ]
            }
        }

    },
    "worker:flair.app": {
        "boot": {
            "load": [
                "./app/preamble.js",
                "flair.boot.DIContainer"
            ]
        }
    }
}