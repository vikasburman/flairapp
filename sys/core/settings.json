{
    "catalog": {
        "Base": "sys.core.Base",
        "Bootstrapper": "sys.core.boot.Server | sys.core.boot.Client",
        "Main": "app.main.Application | web.main.Application"
    },

    "container": {
    },

    "settings": {
        "port": {
            "dev": 80,
            "prod": 443
        },
        "ssl": {
            "public": "",
            "private": ""
        },
        "subdomains": {
            "www": {
                "response": {
                    "headers": [
                        { "name": "Access-Control-Allow-Credentials", "value": true },
                        { "name": "Access-Control-Allow-Origin", "value": "*" },
                        { "name": "Access-Control-Allow-Methods", "value": "GET, PUT, POST, DELETE" },
                        { "name": "Access-Control-Allow-Headers", "value": "X-Requested-With, X-HTTP-Method-Override, Content-Type, Accept, Request" }
                    ]
                }
            }
        },
        "bootwares": []
    }
}