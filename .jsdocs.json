{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "opts": {
        "destination": "docs",
            "tutorials": "gears/tutorials",
            "template": "node_modules/minami",
            "encoding": "utf8",
            "private": true,
            "recurse": true            
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": true,
        "showInheritedInNav": true,
        "default": {
            "outputSourceFiles": true
        }
    }
}