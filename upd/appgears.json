{
    "title": "appgears Framework", 
    "downloader": "github",
    "condition": {
        "allowedIn": "",
        "notAllowedIn": "appgears"
    },
    "url": "https://github.com/",
    "token": "",
    "repo": "vikasburman/appgears",
    "branch": "master",
    "package": {
        "versionKey": "agversion",
        "devDependencies": true,
        "dependencies": true
    },
    "update": {
        "folders": [
            { "src": "app/modules/sample", "dest": "app/modules/sample" },
            { "src": "web/modules/sample", "dest": "web/modules/sample" },
            { "src": "sys/modules/core", "dest": "sys/modules/core" },
            { "src": "sys/modules/node_modules", "dest": "sys/modules/node_modules" },
            { "src": "build", "dest": "build" }
        ],
        "files": [
            { "name": "gulpfile.js", "src": "", "dest": "" },
            { "name": "update.js", "src": "upd/", "dest": "upd/" },
            { "name": "oojs.json", "src": "upd/", "dest": "upd/" }, 
            { "name": "appgears.json", "src": "upd/", "dest": "upd/" }
        ]
    }
}